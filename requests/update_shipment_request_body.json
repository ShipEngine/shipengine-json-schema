            "default": [
              {
                "currency": "usd",
                "amount": 0
              }
            ],
            "title": "monetary_value",
            "type": "object",
            "required": [
              "currency",
              "amount"
            ],
            "additionalProperties": false,
            "properties": {
              "currency": {
