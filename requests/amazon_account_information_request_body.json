{
  "$id": "amazon_account_information_request_body",
  "title": "amazon_account_information_request_body",
  "type": "object",
  "properties": {
    "store_name": {
      "type": "string"
    },
    "merchant_seller_id": {
      "title": "se_id",
      "type": "string",
      "minLength": 1,
      "maxLength": 25,
      "pattern": "^se(-[a-z0-9]+)+$"
    },
    "product_identifier": {
      "title": "amazon_product_identifier",
      "type": "string",
      "enum": [
        "sku",
        "asin"
      ]
    },
    "mws_auth_token": {
      "type": "string"
    }
  }
}