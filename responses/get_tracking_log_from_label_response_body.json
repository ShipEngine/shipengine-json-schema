              "city_locality": {
                "type": "string",
                "minLength": 0
              },
              "state_province": {
                "type": "string",
                "minLength": 2
              },
              "postal_code": {
                "type": "string",
                "minLength": 5
              },
              "country_code": {
                "allOf": [
                  {
                    "title": "country_code",
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 2
                  }
                ]
              },
              "company_name": {
                "type": "string",
                "minLength": 0
              },
              "signer": {
                "type": "string",
                "minLength": 0
              },
              "event_code": {
                "type": "string",
                "minLength": 0
              }
            }
          }
        }
