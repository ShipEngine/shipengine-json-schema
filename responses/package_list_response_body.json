{
  "$id": "package_list_response_body",
  "title": "package_list_response_body",
  "type": "object",
  "properties": {
    "packages": {
      "type": "array",
      "items": {
        "title": "package",
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "package_id": {
            "title": "se_id",
            "type": "string",
            "minLength": 1,
            "maxLength": 25,
            "pattern": "^se(-[a-z0-9]+)+$"
          },
          "package_code": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "dimensions": {
            "title": "dimensions",
            "type": "object",
            "properties": {
              "unit": {
                "title": "dimension_unit",
                "type": "string",
                "enum": [
                  "inch",
                  "centimeter"
                ]
              },
              "length": {
                "type": "number",
                "format": "double"
              },
              "width": {
                "type": "number",
                "format": "double"
              },
              "height": {
                "type": "number",
                "format": "double"
              }
            }
          },
          "description": {
            "type": "string"
          }
        }
      }
    }
  }
}