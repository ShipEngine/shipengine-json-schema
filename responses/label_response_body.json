            "type": "object",
            "properties": {
              "value": {
                "type": "number",
                "format": "double"
              },
              "unit": {
                "title": "weight_unit",
                "type": "string",
                "enum": [
                  "pound",
                  "ounce",
                  "gram",
                  "kilogram"
                ]
              }
            }
          },
          "dimensions": {
            "additionalProperties": false,
            "title": "dimensions",
            "type": "object",
            "properties": {
              "unit": {
                "title": "dimension_unit",
