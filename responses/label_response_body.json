      "additionalProperties": false,
      "required": [
        "href"
      ],
      "properties": {
        "href": {
          "title": "url",
          "type": "string",
          "format": "url",
          "minLength": 1
        },
        "type": {
          "type": "string",
          "minLength": 1
        }
      }
