  "title": "label_response_body",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "label_id",
    "status",
    "shipment_id",
    "ship_date",
    "created_at",
    "shipment_cost",
    "insurance_cost",
    "tracking_number",
    "is_return_label",
    "rma_number",
    "is_international",
    "batch_id",
    "carrier_id",
    "service_code",
    "package_code",
    "voided",
    "voided_at",
    "label_format",
    "label_layout",
    "trackable",
    "carrier_code",
    "tracking_status",
    "label_download",
    "form_download",
    "insurance_claim",
